def найти_номер(список, номер_автомобиля):
    if номер_автомобиля in список:
        return f"Номер {номер_автомобиля} найден в списке."
    else:
        return "Номер не найден в списке."

список_номеров = ["А124ВС", "О777ОО", "М555ММ", "Т888ТТ"]
номер_автомобиля = "М555ММ"

результат_поиска = найти_номер(список_номеров, номер_автомобиля)
print(результат_поиска)
